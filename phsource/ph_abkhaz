// Минимальные фонемы для теста
phoneme a
ipa a
vwl starttype #a endtype #a
length 150
IF thisPh(isNotStressed) THEN
IF nextVowel(isStressed) THEN
FMT(vowel/a#_4)
ELSE
FMT(vowel/V_3)
ENDIF
ENDIF
FMT(vowel/a)
endphoneme




phoneme b
ipa b
vcd vel stp
length 120
FMT(b/b_)
endphoneme

phoneme v
ipa v
vcd frc
length 120
FMT(klatt/v_)
endphoneme

phoneme g
ipa g
vcd vel stp
length 120
FMT(g/g_)
endphoneme

phoneme gj
ipa gʲ
vcd  vel stp  pzd               // palatalized
length 130
FMT(g/g_) addWav(vowel/i, 5)
endphoneme

phoneme gw
ipa gʷ
vcd  vel stp  pzd               // palatalized
length 130
FMT(g/g_) addWav(vowel/u, 5)
endphoneme


phoneme R
ipa ʁ
vcd  uvl trl liquid rhotic trl
length 110
FMT(vwl_fr/r)
endphoneme

phoneme Rj
ipa ʁʲ
vcd   trl frc pzd
length 130
FMT(vwl_fr/rj)
endphoneme

phoneme Rw
ipa ʁʷ
vcd  vel stp  pzd
endphoneme

phoneme d
ipa d
vcd  vel stp  pzd
endphoneme

